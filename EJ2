
//Vamos a encontrar al usuario m치s joven
let menorEdad = edadesMayores.reduce(function(menor){
    menor = edadesMayores[0];//menor ser치 igual a la primera edad

    for(i=0;i<=edadesMayores.length;i++){
        if(edadesMayores[i]<menor) menor = edadesMayores[i];//Si la edad actual del bucle es menor que la guardada en la variable menor, cambia su valor
    }
    return menor;//devuelve menor
});

console.log("El usuario m치s joven tiene "+menorEdad+" a침os");

//Vamos a sacar los usuarios que empiezan con A

const nombresConA = usuarios.filter(usuario => usuario.nombre.startsWith('A')).map(usuario => usuario.nombre);


console.log("Usuarios que empiezan por A: "+nombresConA);

